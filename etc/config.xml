<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <silin_authorizenet>
                <active>1</active>
                <title>Authorize.Net Payment</title>
                <model>SilinAuthorizenetFacade</model>
                <can_use_checkout>1</can_use_checkout>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <cctypes>VI,MC</cctypes>
                <payment_action>authorize_capture</payment_action>
                <debug>true</debug>
                <paymentInfoKeys>auth_code,avs_result_code,cv_result_code,cavv_result_code,account_number,account_type</paymentInfoKeys>
                <privateInfoKeys>auth_code,avs_result_code,cv_result_code,cavv_result_code</privateInfoKeys>
            </silin_authorizenet>
        </payment>
    </default>
</config>
